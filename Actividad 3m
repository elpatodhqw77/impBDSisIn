mysql> INSERT INTO Permutaciones (Caso)
    -> VALUES
    -> ('A'),
    ->    ('B'),
    ->    ('C');
Query OK, 3 rows affected (0.01 sec)
Records: 3  Duplicates: 0  Warnings: 0

mysql> SELECT 
    ->     CONCAT(P1.Caso, ',', P2.Caso, ',', P3.Caso) AS RESULTADO
    -> FROM 
    ->     Permutaciones P1
    -> JOIN 
    ->     Permutaciones P2 ON t1.Caso <> P2.Caso
    -> JOIN 
    ->     Permutaciones P3 ON t1.Caso <> P3.Caso AND P2.Caso <> 
    -> P3.Caso 
    -> ORDER BY 
    ->     RESULTADO;
ERROR 1054 (42S22): Unknown column 't1.Caso' in 'on clause'
mysql> SELECT 
    ->     CONCAT(P1.Caso, ',', P2.Caso, ',', P3.Caso) AS RESULTADO
    -> FROM 
    ->     Permutaciones P1
    -> JOIN 
    ->     Permutaciones P2 ON P1.Caso <> P2.Caso
    -> JOIN 
    ->     Permutaciones P3 ON P1.Caso <> P3.Caso AND P2.Caso <> 
    -> P3.Caso 
    -> ORDER BY 
    ->     RESULTADO;
